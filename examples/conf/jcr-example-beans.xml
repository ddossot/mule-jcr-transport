<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="jcrRepository" class="org.apache.jackrabbit.core.TransientRepository" />
	<bean id="contentGenerator" class="org.mule.examples.jcr.JcrContentLoader" init-method="initialize">
		<property name="repository">
			<ref bean="jcrRepository" />
		</property>
		<property name="username">
			<value>admin</value>
		</property>
		<property name="password">
			<value>admin</value>
		</property>
		<property name="rootNodeName">
			<value>example</value>
		</property>
	</bean>
</beans>

